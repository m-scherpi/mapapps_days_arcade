{
    "name": "arcade-executor",
    "version": "0.0.1",
    "dependencies": {
        "esri": "^4.24.7",
        "apprt-vue": "^4.14.0",
        "apprt-vuetify": "^4.14.0",
        "apprt-core": "^4.14.0",
        "map-widget": "^4.14.0"
    },
    "main": "",
    "i18n": ["bundle"],
    "layer": "module",
    "layout-widgets": [
        {
            "widgetRole": "arcade",
            "template": "seasons",
            "window": {
                "autofocus": false,
                "dockTool": "arcadeTool",
                "minimizeOnClose": true,
                "marginBox": {
                    "r": 0,
                    "t": 0,
                    "b": 0,
                    "w": 500
                }
            }
        }
    ],
    "components": [
        {
            "name": "Config",
            "provides": ["dijit.Widget"],
            "instanceFactory": true,
            "properties": {
                "widgetRole": "arcade"
            },
            "references": [
                {
                    "name": "mapWidgetModel",
                    "providing": "map-widget.MapWidgetModel"
                }
            ]
        },
        {
            "name": "ArcadeTool",
            "impl": "ct/tools/Tool",
            "provides": [
                "ct.tools.Tool"
            ],
            "propertiesConstructor": true,
            "properties": {
                "id": "arcadeTool",
                "title": "${window.title}",
                "tooltip": "${window.tooltip}",
                "iconClass": "icon-star-o",
                "toolRole": "toolset",
                "togglable": true,
                "rules": {
                    "noGroup": true
                }
            }
        }
    ]
}
